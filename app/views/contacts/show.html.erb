<h1>Contact Info</h1>

<h2>First Name: <%= @contact.first_name %></h2>
<h2>Last Name: <%= @contact.last_name %></h2>
<h2>Email: <%= @contact.email %></h2>
<h2>Phone Number: <%= @contact.phone_number %></h2>

<%= link_to "Edit Contact", "/contacts/#{ @contact.id }/edit", class: "btn btn-warning" %>
<%= link_to "Delete Contact", "/contacts/#{ @contact.id }", method: :delete, class: "btn btn-danger" %>